@model FisioTerapias.Models.Terapia

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<h3>¿Está seguro de que desea eliminar esto?</h3>
<div>
    <h4>Terapia</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Fecha)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Diagnostico)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Diagnostico)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Historial)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Historial)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NecesidadDelPaciente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NecesidadDelPaciente)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CantidadDeTerapias)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CantidadDeTerapias)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdEmpleadoNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdEmpleadoNavigation.IdEmpleado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdEstadoTerapiaNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdEstadoTerapiaNavigation.IdEstadoTerapia)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdPacienteNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdPacienteNavigation.IdPaciente)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdTerapia" />
        <input type="submit" value="Eliminar" class="btn btn-danger" />
        <a class="btn btn-secondary" asp-action="Index">Volver a la Lista</a>
    </form>
</div>
